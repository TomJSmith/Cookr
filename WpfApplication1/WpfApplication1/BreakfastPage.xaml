                <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Height="111" Margin="75,1236,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="526" FontSize="16"><Run Text="In a large bowl, mix"/><Run Text=" using a whisk,"/><Run Text=" flour, sugar, baking powder"/><Run Text=", "/><Run Text="salt."/><Run Text=", "/><Run Text="milk, egg and oil. Mix until smooth."/></TextBlock>
                <Label x:Name="label5" Content="Step 1" HorizontalAlignment="Left" Height="36" Margin="1,1231,0,0" VerticalAlignment="Top" Width="69" FontSize="18.667"/>
                <Rectangle HorizontalAlignment="Left" Margin="0,1347,0,617" Width="620"/>
                <Image x:Name="image12_Copy" HorizontalAlignment="Left" Height="45" Margin="9,1388,0,0" VerticalAlignment="Top" Width="60" Source="Pics/making pancakes.jpg"/>
                <TextBlock x:Name="textBlock1_Copy" HorizontalAlignment="Left" Height="111" Margin="70,1352,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="526" FontSize="16"><Run Text="Heat a lightly oiled frying pan over medium high heat. Using the ladle, scoop "/><Run Text="the batter onto the "/><Run Text="pan"/><Run Text=", using approximately 1/4 cup for each pancake. Let it cook for 3 minutes or until the bottom is golden brown. Flip the pancake and let the other side cook for one minute. Serve hot!"/></TextBlock>
                <Label x:Name="label5_Copy" Content="Step 2" HorizontalAlignment="Left" Height="36" Margin="1,1347,0,0" VerticalAlignment="Top" Width="69" FontSize="18.667"/>
                <Grid HorizontalAlignment="Left" Height="52" Margin="133,1468,0,0" VerticalAlignment="Top" Width="345"/>
                <Image x:Name="image6" HorizontalAlignment="Left" Height="45" VerticalAlignment="Top" Width="45" Source="Images/backArrow.png" RenderTransformOrigin="1,1.667" Margin="0,3,0,0" MouseUp="goBack"/>
                <Image x:Name="favButton" HorizontalAlignment="Left" Height="45" Margin="133,174,0,0" VerticalAlignment="Top" Width="45" Source="Images/addFavouritedButton.png" MouseUp="addFavourite"/>
                <Image x:Name="image4_Copy6" HorizontalAlignment="Left" Height="60" Margin="9,955,0,0" VerticalAlignment="Top" Width="60" Source="Pics/eggs.jpg"/>
                <Label x:Name="label4_Copy2" Content="1 large egg" HorizontalAlignment="Left" Height="60" Margin="75,955,0,0" VerticalAlignment="Top" Width="232" FontSize="18.667"/>
                <Label x:Name="label4_Copy3" Content="2 tablespoons canola&#xD;&#xA;oil" HorizontalAlignment="Left" Height="60" Margin="75,1032,0,0" VerticalAlignment="Top" Width="232" FontSize="18.667"/>
                <Image x:Name="image4_Copy7" HorizontalAlignment="Left" Height="60" Margin="9,1032,0,0" VerticalAlignment="Top" Width="60" Source="Pics/Canola-Oil.jpg"/>
                <Image Margin="323,630,237,1384" Source="Pics/Large mixing bowl.jpg"/>
                <Label x:Name="label4_Copy13" Content="Mixing Bowl" HorizontalAlignment="Left" Height="43" Margin="393,639,0,0" VerticalAlignment="Top" Width="122" FontSize="18.667"/>
                <local:UserRating Margin="143,1401,142,-69"></local:UserRating>


                <Label x:Name="label4_Copy15" Content="Ladle" HorizontalAlignment="Left" Height="43" Margin="75,640,0,0" VerticalAlignment="Top" Width="151" FontSize="18.667"/>
                <Image x:Name="image4_Copy12" HorizontalAlignment="Left" Height="60" Margin="10,632,0,0" VerticalAlignment="Top" Width="60" Source="Pics/ladle-big.jpg"/>
                <Image Margin="10,692,550,1322" Source="Pics/-measuring-cup-set-1-600x600.jpg"/>
                <Label x:Name="label4_Copy4" Content="Measuring cups" HorizontalAlignment="Left" Height="43" Margin="75,698,0,0" VerticalAlignment="Top" Width="151" FontSize="18.667"/>


                <Image x:Name="image5" Source="Pics/Large Mixing Bowl.jpg" HorizontalAlignment="Left" Height="76" Margin="324,635,0,0" VerticalAlignment="Top" Width="59"/>
                <Label x:Name="label4_Copy6" Content="Egg Flipper" HorizontalAlignment="Left" Height="43" Margin="393,701,0,0" VerticalAlignment="Top" Width="122" FontSize="18.667"/>
                <Image x:Name="image5_Copy1" Source="Pics/eggFlipper.jpg" HorizontalAlignment="Left" Height="76" Margin="324,693,0,0" VerticalAlignment="Top" Width="59"/>

            </Grid>
        </ScrollViewer>


    </Grid>
