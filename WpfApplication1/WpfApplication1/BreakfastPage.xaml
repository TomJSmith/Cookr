                <Label x:Name="label4_Copy9" Content="Baking pouder" HorizontalAlignment="Left" Height="65" Margin="388,863,0,0" VerticalAlignment="Top" Width="232" FontSize="18.667"/>
                <Image x:Name="image4_Copy5" HorizontalAlignment="Left" Height="60" Margin="323,955,0,0" VerticalAlignment="Top" Width="60" Source="Pics/milk.jpg"/>
                <Label x:Name="label4_Copy10" Content="1 cup milk" HorizontalAlignment="Left" Height="60" Margin="389,955,-1,0" VerticalAlignment="Top" Width="232" FontSize="18.667"/>
                <Rectangle Fill="#FFE8685D" Margin="1,1231,-1,0" Stroke="#FFE8685D" Height="116" VerticalAlignment="Top"/>
                <Image x:Name="image12" HorizontalAlignment="Left" Height="60" Margin="9,1267,0,0" VerticalAlignment="Top" Width="60" Source="Pics/whisking.jpg"/>
                <Border BorderBrush="#FFE8685D" BorderThickness="1" HorizontalAlignment="Left" Height="327" Margin="0,1136,-1,0" VerticalAlignment="Top" Width="621"/>
